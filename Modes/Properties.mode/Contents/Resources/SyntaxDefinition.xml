<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE syntax SYSTEM "syntax.dtd">
<syntax>
	<head>
		<name>Properties</name>
		<charsintokens><![CDATA[_0987654321abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ@]]></charsintokens>
	</head>
	<states>
		<default id="Base" color="#000000" background-color="#ffffff" font-weight="normal" font-style="normal">
      <keywords id="Numbers" color="#00f" font-style="normal" font-weight="normal" useforautocomplete="no">
        <regex>(?:\ ([\+\-\.]?[0-9\.]+)(?:\ |$))</regex>
        <regex>(?:\=[\ ]*([\+\-\.]?[0-9\.]+)(?:\ |$))</regex>
			</keywords>
			
      <keywords id="Literals" color="#a1617a" font-style="normal" font-weight="normal" useforautocomplete="yes">
        <string>false</string>
        <string>true</string>
      </keywords>
      
			<state id="Comment" type="comment" color="#236E25" font-weight="normal" font-style="normal">
				<begin><regex>^[\ ]*[#|!]</regex></begin>
				<end><regex>[\n\r]</regex></end>
			</state>
		</default>
	</states>
</syntax>
